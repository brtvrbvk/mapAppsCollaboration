{
    "dataform-version": "1.0.0",
//    "size": {
//        "h": 200,
//        "w": 500
//    },
    "type": "gridpanel",
    "showLabels": true,
    "cols": 1,
    "children": [
        {
            "type": "numbertextbox",
            "title": "${menu.geolocator.count}",
            "field": "geolocatorSuggestQueryOptions.count",
            "min": 0,
            "max": 100
        },
        {
            "type": "numbertextbox",
            "title": "${menu.geolocator.minQueryLength}",
            "field": "geolocatorSuggestStoreParameters.minQueryLength",
            "min": 0,
            "max": 100
        },
        {
            "type": "checkbox",
            "title": "${menu.geolocator.useNavteqFallback}",
            "field": "useNavteqFallback"
        }

    ]
}