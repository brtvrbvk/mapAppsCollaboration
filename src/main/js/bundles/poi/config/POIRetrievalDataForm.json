{
    "dataform-version": "1.0.0",
    "size": {
        "h": 200,
        "w": 500
    },
    "type": "gridpanel",
    "showLabels": true,
    "cols": 1,
    "children": [
        {
            "type": "numbertextbox",
            "title": "${menu.symbolization.maxcount}",
            "field": "maxcount",
            "min": 1,
            "max": 10000000
        },
        {
            "type": "checkbox",
            "title": "${menu.symbolization.clusterlabels}",
            "field": "showClusterLabels"
        },
        {
            "type": "numbertextbox",
            "title": "${menu.scale}",
            "field": "symbolFactoryOpts.generalizationScale",
            "min": 1,
            "max": 10000000
        },
        {
            "type": "numbertextbox",
            "title": "Resultcount threshold",
            "field": "symbolFactoryOpts.generalizationThreshold",
            "min": 1,
            "max": 1024
        },
        {
            "type": "checkbox",
            "title": "Differ in vis. depending on scale/resultcount",
            "field": "symbolFactoryOpts.doGeneralization"
        },
        {
            "type": "textarea",
            "title": "${menu.symbolization.icons}",
            "field": "symbolFactoryOpts.lookupTable",
            "size": {
                "w": 500,
                "h": 250
            }
        },
        {
            "type": "textarea",
            "title": "${menu.symbolization.texts}",
            "field": "clusterLookupTable",
            "size": {
                "w": 500,
                "h": 250
            }
        }
    ]
}