{
    "dataform-version": "1.0.0",
    "size": {
        "h": 200,
        "w": 500
    },
    "type": "gridpanel",
    "showLabels": true,
    "cols": 1,
    "children": [
        {
            "type": "checkbox",
            "title": "${menu.symbolization.clusterlabels}",
            "field": "showClusterLabels"
        },
        {
            "type": "textarea",
            "title": "${menu.symbolization.icons}",
            "field": "lookupTable",
            "size": {
                "w": 500,
                "h": 250
            }
//            "wrap": "on"
        },
        {
            "type": "textarea",
            "title": "${menu.symbolization.texts}",
            "field": "clusterLookupTable",
            "size": {
                "w": 500,
                "h": 250
            }
//            "wrap": "on"
        }
    ]
}