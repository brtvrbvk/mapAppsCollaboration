{
    "Bundle-SymbolicName": "coordinateparser",
    "Bundle-Version": "5.0.4",

    "Bundle-Name": "coordinateparser",
    "Bundle-Description": "",
    "Bundle-Vendor": "con terra GmbH",
    "Bundle-Category": [""],

    //"Bundle-ExcludedExecutionEnvironment" : ["iPhone","iPad","Android"],
    "Bundle-Icon": {},
    "Bundle-License": {},

    //"Config-Bundle-Location": "/config",
    "Bundle-AutoStartPolicy": "yes",
    "Require-Bundle": [

    ],
    "Components": [
        
        /*
         * WIDGET CONTROLLER
         */
        {
            "name": "CoordinateWidgetController",
            "provides": [
                "search.CoordinateWidgetController",
                "ct.framework.api.EventHandler"
            ],
            "propertiesConstructor": true,
            "properties": {
                "searchDelay": 300,
                "removeResultsOnClear": false,
                "Event-Topics": [
                    {
                        "topic": "agiv/search/handler/START",
                        "method": "_addHandlerWorker"
                    },
                    {
                        "topic": "agiv/search/handler/COMPLETE",
                        "method": "_removeHandlerWorker"
                    },
                    {
                        "topic": "agiv/search/result/SELECTED_RESULT",
                        "method": "_handleResultSelection"
                    },
                    {
                        "topic": "agiv/search/VALUE_CHANGE",
                        "method": "_handleNewSearch"
                    },
                    {
                        "topic": "agiv/search/result/CLOSE_WINDOW",
                        "method": "_closeWindow"
                    },
                    {
                        "topic": "agiv/search/result/NEW",
                        "method": "_handleNewResult"
                    }
                ]
            },
            "references": [
                {
                    "name": "eventService",
                    "providing": "ct.framework.api.EventService"
                },
                {
                    "name": "coordinateParsers",
                    "providing": "ct.api.search.CoordinateParser",
                    "cardinality": "0..n"
                },
                {
                    "name": "widget",
                    "providing": "search.SearchWidget"
                }
            ]
        },
        
        
        /*
         * COORDINATE PARSERS
         */
        {
            "name": "WGS84Parser",
            "provides": [
                "ct.framework.api.EventHandler"
            ],
            "propertiesConstructor": true,
            "properties": {
                "priority": 35,
                "template": "${y}\u00B0 - ${x}\u00B0",
                "decimals": 4,
                "Event-Topics": [
                    {
                        "topic": "agiv/search/VALUE_CHANGE",
                        "method": "_handleValueChange"
                    },
                    {
                        "topic": "agiv/search/result/REHANDLE",
                        "method": "_rehandleResult"
                    }
                ]
            },
            "references": [
                {
                    "name": "eventService",
                    "providing": "ct.framework.api.EventService"
                }
            ]
        },

        {
            "name": "BelgianLambert72Parser",
            "provides": [
                "ct.framework.api.EventHandler"
            ],
            "propertiesConstructor": true,
            "properties": {
                "priority": 36,
                "template": "${x} m - ${y} m",
                "decimals": 2,
                "Event-Topics": [
                    {
                        "topic": "agiv/search/VALUE_CHANGE",
                        "method": "_handleValueChange"
                    },
                    {
                        "topic": "agiv/search/result/REHANDLE",
                        "method": "_rehandleResult"
                    }
                ]
            },
            "references": [
                {
                    "name": "eventService",
                    "providing": "ct.framework.api.EventService"
                }
            ]
        }
    ]
}